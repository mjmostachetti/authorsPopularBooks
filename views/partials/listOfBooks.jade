.container
  .row
    .col-md-12.text-center
      h1 Author Data Analysis
      form.text-center
        input#searchAuthor(ng-keyup="onKeyUp($event)")
        input(type='button' value="Click Here" ng-click='search()')
  .row
    .col-md-6.text-center
      h2!='Author : {{ authorObj.name[0] }}'
      .row
        .col-md-12
          img#mainAuthorPic(ng-src='{{ authorPic }}')
    .col-md-6.text-center
      h3#bookTitle!='{{ title }}'
      .row
        .col-md-12#descDiv
          p#bookDesc!='{{ bookDesc }}'
  .row
    #svgDocker.col-md-12.text-center
  .row
    .col-md-12
      img(ng-repeat="inspiration in influencesToAuthor" src!='{{ inspiration.image_url[0] }}')
      p(ng-repeat="inspiration in influencesToAuthor")!='{{ inspiration.name[0] }}'


